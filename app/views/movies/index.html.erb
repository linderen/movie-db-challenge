<div class="page-header">
  <h1>Mine film</h1>
</div>
<div class="row">
  <% @movies.each do |movie| %>
  <%= link_to movie do %>
  <div class="col-md-3 movie-thumbnail" data-url="<%= movie_url(movie) %>">
    <div class="row"><%= movie.title %></div>
    <div class="row"><%= movie_thumbnail(movie) %></div>
  </div>
  <% end end %>
</div>
